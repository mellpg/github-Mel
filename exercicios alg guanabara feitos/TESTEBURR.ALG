algoritmo "testeburr"
var

   responda,soma:inteiro

inicio

   escreva("quanto e a soma de 2 + 2?")
   leia(responda)
   soma<- 2+2
   se soma=4 entao
   escreval("parabens voce nao e burro")
   
fimse

   se soma <> 4 entao
   escreval("voce e burro pra caramba")


fimse
   
   
   
   

fimalgoritmo